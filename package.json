{
  "name": "congmaps2",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "_____ EXPO EAS _____": "_",
    "eas:upd:preview": "npx eas-cli update --branch preview",
    "eas:build:android": "npx eas-cli build -p android --profile preview",
    "eas:build:ios": "npx eas-cli build -p ios --profile preview",
    "eas:submit:ios": "npx eas-cli submit --platform ios",
    "_____ EXPO _____": "_",
    "start": "npx expo start",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "webCodeSpaces": "yarn setCodeSpaces && yarn web",
    "pub": "npx expo publish",
    "export:web:clear": "npx expo export:web --clear",
    "web:clear": "npx expo start --clear --web",
    "start:clear": "npx expo start --clear",
    "eject": "npx expo eject",
    "build:clear": "npx expo build:web --clear",
    "_____ FIREBASE _____": "_",
    "deploy:complete": "yarn export:web:clear && yarn deploy:hosting",
    "deploy:hosting": "npx firebase-tools deploy --only hosting",
    "firebase:login": "npx firebase-tools login",
    "firebase:login:ci": "npx firebase-tools login --no-localhost"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.0",
    "@expo/webpack-config": "^19.0.0",
    "@googlemaps/react-wrapper": "^1.1.35",
    "@googlemaps/typescript-guards": "^2.0.3",
    "@morfos/central-data": "^0.0.25",
    "@morfos/renders": "^0.0.12",
    "@morfos/routes": "^0.0.18",
    "@react-native-picker/picker": "^2.4.8",
    "expo": "^49.0.21",
    "expo-document-picker": "~9.2.4",
    "expo-updates": "~0.18.19",
    "fast-equals": "^4.0.3",
    "firebase": "^9.14.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.4",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "typescript": "^5.1.3"
  },
  "private": true
}
